
{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/client"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/dist/client/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}
